#!/bin/env python 3.3.2
import collections
import re

MinRelDates = ['20050104', '20050223', '20050412', '20050524', '20050721', '20050830', '20051011', '20051122', '20060103',
               '20060221', '20060418', '20060531', '20060720', '20060829', '20061011', '20061115', '20070103', '20070221',
               '20070411', '20070530', '20070719', '20070828', '20071009', '20071120', '20080102', '20080220', '20080408',
               '20080521', '20080716', '20080826', '20081007', '20081119', '20090106', '20090218', '20090408', '20090520',
               '20090715', '20090902', '20091014', '20091124', '20100106', '20100217', '20100406', '20100519', '20100714',
               '20100831', '20101012', '20101123', '20110104', '20110216', '20110405', '20110518', '20110712', '20110830',
               '20111012', '20111122', '20120103', '20120215', '20120403', '20120516', '20120711', '20120822', '20121004',
               '20121114', '20130103', '20130220', '20130410', '20130522', '20130710', '20130821', '20131009', '20131120',
               '20140108', '20140219', '20140409', '20140521', '20140709', '20140820', '20141008', '20141119', '20150107',
               '20150218', '20150408', '20150520', '20150708', '20150819', '20151008', '20151118', '20160106']
               


wanted = set(['eased'])
cnt = collections.Counter()

for doc in MinRelDates:
    words = re.findall('\w+', open(doc+'.txt').read().lower())
    for word in words:
        if word in wanted:
            cnt [word] += 1
    print (cnt)
